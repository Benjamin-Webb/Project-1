\begin_layout Standard
\begin_inset Formula $\mathrm{x\left(t+1\right)=x\left(t\right)+\dot{x}\left(t\right)\Delta t}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathrm{\dot{x}\left(t+1\right)=\dot{x}\left(t\right)+\ddot{x}\left(t\right)\Delta t}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathrm{y\left(t+1\right)=y\left(t\right)+\dot{\dot{y}\left(t\right)\Delta t}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathrm{\dot{y}\left(t+1\right)=\dot{y}\left(t\right)+\ddot{y}\left(t\right)\Delta t}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathrm{\phi\left(t+1\right)=\text{\ensuremath{\phi}}\left(t\right)+\ddot{x}\left(t\right)\Delta t}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbf{X}\mathrm{\left(t\right)=\begin{bmatrix}x\left(t\right) & \dot{x}\left(t\right) & y\left(t\right) & \dot{y}\left(t\right) & \phi\left(t\right)\end{bmatrix}^{T}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbf{U}\mathrm{\left(t\right)=\begin{bmatrix}\ddot{y}\left(t\right) & \ddot{x}\left(t\right)\end{bmatrix}^{T}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbf{U}\mathrm{\left(t\right)=\mathit{f_{\theta}}\left(\mathbf{X}\left(t\right)\right)}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathrm{\underset{\theta}{min}\;\;\left\Vert \mathbf{X}\left(T\right)\right\Vert ^{2}=x\left(T\right)^{2}+\dot{x}\left(T\right)^{2}+y\left(T\right)^{2}+\dot{y}\left(T\right)^{2}+\phi\left(T\right)^{2}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
\mathrm{s.t.\;\;\;\;\:x\left(t+1\right)=x\left(t\right)+\dot{x}\left(t\right)\Delta t}\\
\mathrm{\;\;\;\;\;\dot{x}\left(t+1\right)=\dot{x}\left(t\right)+\ddot{x}\left(t\right)\Delta t}\\
\;\;\;\;\;\mathrm{y\left(t+1\right)=y\left(t\right)+\dot{\dot{y}\left(t\right)\Delta t}}\\
\;\;\;\;\;\mathrm{\dot{y}\left(t+1\right)=\dot{y}\left(t\right)+\ddot{y}\left(t\right)\Delta t}\\
\;\;\;\;\;\mathrm{\phi\left(t+1\right)=\text{\ensuremath{\phi}}\left(t\right)+\ddot{x}\left(t\right)\Delta t}\\
\mathbf{U}\mathrm{\left(t\right)=\mathit{f_{\theta}}\left(\mathbf{X}\left(t\right)\right)},\:\mathrm{\forall t=1,...,T-1}
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathrm{x\left(T\right)=0,\,\dot{x}\left(T\right)=0,\,y\left(T\right)=0,\,\dot{y}\left(T\right)=0,\,and\,\phi\left(T\right)=0}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathrm{\dot{x}:\;0.0}$
